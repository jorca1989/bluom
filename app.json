{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bluom",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "enabled": false
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jwfca.bluom",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow access to your camera to recognize foods from photos."
      }
    },
    "android": {
      "package": "com.jwfca.bluom"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow access to your camera to recognize foods from photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}